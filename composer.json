{
    "name": "pentagonal/modular",
    "description": "Easy php module storage for small project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pentagonal",
            "email": "org@pentagonal.org",
            "role" : "developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-tokenizer": "*",
        "pentagonal/array-store": "~1",
        "pentagonal/php-evaluator": "~1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^6",
        "satooshi/php-coveralls": "^1"
    },
    "autoload" : {
        "psr-4" : {
            "Pentagonal\\Modular\\": "Src/"
        },
        "classmap": [
            "Src/"
        ]
    }
}
